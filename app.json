{
  "expo": {
    "name": "vizinho-agro-app",
    "slug": "vizinho-agro-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "vizinhoagroapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
       "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "navigationBar": {
        "backgroundColor": "#D9D9D9",
        "barStyle": "dark-content"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
